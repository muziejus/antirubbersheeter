{
  "name": "antirubbersheeter-server",
  "version": "0.0.0",
  "description": "A node-based server for the antirubbersheeter",
  "private": true,
  "scripts": {
    "build:babel": "NODE_ENV=production babel --config-file ./babel.config.js ./src --extensions .ts --out-dir ./dist",
    "build:tsc": "tsc --build",
    "build": "pnpm run clean && npm-run-all build:babel build:tsc",
    "clean": "rimraf dist/*",
    "lint": "eslint --color --format=pretty . --cache",
    "lint:fix": "eslint --color --format=pretty . --fix",
    "start": "pnpm run build && node dist/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon"
  },
  "dependencies": {
    "archiver": "^5.3.1",
    "body-parser": "^1.19.2",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-fileupload": "^1.3.1",
    "morgan": "^1.10.0",
    "sharp": "^0.30.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/archiver": "^5.3.1",
    "@types/busboy": "^1.3.0",
    "@types/cors": "^2.8.12",
    "@types/csv-parse": "^1.2.2",
    "@types/express": "^4.17.13",
    "@types/leaflet": "^1.7.11",
    "@types/morgan": "^1.9.3",
    "@types/sharp": "^0.30.0",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2"
  },
  "volta": {
    "extends": "../package.json"
  }
}
